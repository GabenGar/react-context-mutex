  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/node": "^12.12.25",
    "coveralls": "^3.0.9",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.3.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  },
  "keywords": [
