  "module": "dist/react-context-mutex.es.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/react": "^16.9.17",
    "react": "^16.12.0"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build": "rollup -c",
    "lint": "tslint -p tsconfig.json",
    "clean": "rimraf dist",
